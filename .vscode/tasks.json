{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: c++ build active file",
      "command": "/usr/bin/c++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "-O0",
        "-std=gnu++20",
        "-DDEBUG",
        "-I${fileDirname}/../lib",
        "-I${fileDirname}/../third-party/include",
        "${fileDirname}/*.cpp",
        "${fileDirname}/../lib/*.cpp",
        "-o",
        "${fileDirname}/${relativeFileDirname}.out",
        "-luuid"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
