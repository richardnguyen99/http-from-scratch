{
  "files.associations": {
    "*.ejs": "ejs",
    "iostream": "cpp",
    "http_core.h": "cpp",
    "http_client.h": "cpp",
    "http_server.h": "cpp",
    "http_request.h": "cpp",
    "http_response.h": "cpp",
    "http_config.h": "cpp",
    "blocking_http_server.h": "cpp",
    "multi_process_http_server.h": "cpp",
    "multi_thread_http_server.h": "cpp",
    "cstdbool": "cpp",
    "cstdint": "cpp",
    "cstddef": "cpp"
  },
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/include/**/*",
    "${workspaceFolder}/lib/**/*",
    "/usr/include",
    "/usr/local/include"
  ]
}
