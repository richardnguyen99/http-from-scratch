{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${default}",
        "${workspaceFolder}/lib/**/*",
        "${workspaceFolder}/include/**/*"
      ],
      "defines": ["DEBUG", "_DEBUG"],
      "compilerPath": "/usr/bin/c++",
      "intelliSenseMode": "linux-gcc-x64",
      "configurationProvider": "ms-vscode.cmake-tools",
      "cStandard": "c23",
      "cppStandard": "gnu++20"
    }
  ],
  "version": 4
}
